import math
import os
import random
import re
import sys

# Complete the solve function below.
def solve(s):
    split = s.split()
    f = split[0]
    l = split[1]
    return (f[0].upper() + f[1:] +" "+ l[0].upper() + l[1:])

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    s = input()

    result = solve(s)

    fptr.write(result + '\n')

    fptr.close()
